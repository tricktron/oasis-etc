#!/bin/rc

path=/bin

umask 022

mount -t proc none /proc
mount -t devtmpfs none /dev

mkdir /dev/pts
mkdir /dev/shm
mount -a

insmod input
insmod mousedev
insmod usbcore
insmod xhci_hcd
insmod usbhid
insmod hid_generic
insmod fuse 

/libexec/applyperms
/libexec/applyperms -d /etc

/etc/rc.local

exec perpboot /etc/perp
